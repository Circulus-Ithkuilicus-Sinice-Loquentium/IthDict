        <!-- ROOTS ASSOCIATED WITH ITHKUIL GRAMMATICAL FUNCTIONS -->
        <ar>
            <k>
                <opt>-</opt>
                S
                <opt>-</opt>
            </k>
            <def>
                <def>
                    <gr>
                        <abbr>S0</abbr>
                    </gr>
                    <gr>[the “carrier” root]</gr>
                </def>
                <def>
                    <gr>
                        <abbr>S1</abbr>
                    </gr>
                    <def>
                        <gr>
                            <abbr>BSC</abbr>
                        </gr>
                        <deftext>[carrier stem representing an animate being named by the succeeding foreign or proper name/phrase]</deftext>
                    </def>
